            <xsd:element minOccurs="0" maxOccurs="unbounded" name="REVISIONNUM" nillable="true" type="MXLongQueryType">
              <xsd:annotation>
                <xsd:documentation>Unique Key Component</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="SITEID" type="MXStringQueryType">
