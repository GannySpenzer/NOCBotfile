            <xsd:element minOccurs="0" maxOccurs="unbounded" name="UC_NOPACKSLIP" type="MXBooleanQueryType" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="VENDELIVERYDATE" nillable="true" type="MXDateTimeQueryType" />
            <xsd:element minOccurs="0" name="POLINE">
