                  <xsd:element minOccurs="0" maxOccurs="unbounded" name="VENDELIVERYDATE" nillable="true" type="MXDateTimeQueryType" />
                </xsd:sequence>
                <xsd:attribute name="filter" type="xsd:boolean" use="optional" />
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
